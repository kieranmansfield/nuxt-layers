{
  "name": "nuxt-layers",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "pnpm -r exec rm -rf node_modules .nuxt .output .data && pnpm store prune && pnpm install",
    "cleancache": "pnpm store prune && pnpm store status",
    "update": "npx npm-check-updates -u",
    "nuxtupdate": "npx nuxi@latest upgrade --dedupe --force",
    "nuxiupgrade": "npx nuxi upgrade",
    "update:nuxt-ui": "pnpm up @nuxt/ui --latest --dedupe",
    "update:nuxt-image": "pnpm up @nuxt/image --latest --dedupe",
    "rebuild": "pnpm install && pnpm turbo run build && pnpm turbo run dev"
  },
  "keywords": [],
  "author": "Kieran Mansfield",
  "license": "ISC",
  "packageManager": "pnpm@10.28.2",
  "workspaces": [
    "layers/*",
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@culur/config-stylelint": "^1.6.4",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@iconify-json/lucide": "^1.2.87",
    "@nuxt/eslint": "^1.13.0",
    "@nuxt/image": "^2.0.0",
    "@nuxt/ui": "latest",
    "@nuxtjs/device": "^4.0.0",
    "@perplex-digital/stylelint-config": "^17.2.0",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "^14.1.0",
    "changesets": "^1.0.2",
    "cypress": "^15.9.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.2",
    "eslint-plugin-compat": "^6.1.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^10.7.0",
    "npm-check-updates": "^19.3.2",
    "nuxt": "latest",
    "playwright": "^1.58.0",
    "postcss-html": "^1.8.1",
