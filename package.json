{
  "name": "my-nuxt-layer",
  "type": "module",
  "version": "0.0.1",
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi dev .playground",
    "dev:prepare": "nuxt prepare .playground",
    "build": "nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "lint": "eslint .",
    "clean": "rm -rf ./node_modules package-lock.json .nuxt .output .data && npm cache clean --force && npm i",
    "cleancache": "npm cache clean --force && npm cache verify",
    "browserlist": "npx update-browserslist-db@latest",
    "rebuild": "npm run build && npm run generate && npm run dev",
    "update": "ncu -i",
    "nuxtupdate": "npx nuxi@latest upgrade --dedupe --force",
    "nuxiupgrade": "npx nuxi upgrade"
  },
  "devDependencies": {
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@nuxt/eslint": "latest",
    "@types/node": "^24.10.7",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-vue": "^10.7.0",
    "nuxt": "^4.2.2",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "^5.9.3",
    "vue": "latest",
    "vue-tsc": "^3.2.2"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ]
}
