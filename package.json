    "stylelint-config-standard": "^40.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-config-tailwindcss": "^1.0.1",
    "stylelint-no-unsupported-browser-features": "^8.0.5",
    "stylelint-prettier": "^5.0.3",
    "tailwindcss": "^4.1.18",
    "turbo": "^2.7.6",
    "typescript": "^5.9.3",
    "vite-plugin-checker": "^0.12.0",
