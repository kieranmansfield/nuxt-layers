{
  "name": "my-nuxt-layer",
  "type": "module",
  "version": "0.0.1",
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi dev .playground",
    "dev:prepare": "nuxt prepare .playground",
    "build": "nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "lint": "eslint .",
    "clean": "rm -rf ./node_modules package-lock.json .nuxt .output .data && npm cache clean --force && npm i",
    "cleancache": "npm cache clean --force && npm cache verify",
    "browserlist": "npx update-browserslist-db@latest",
    "rebuild": "npm run dev:prepare && npm run build && npm run generate && npm run dev",
    "update": "ncu -i",
    "nuxtupdate": "npx nuxi@latest upgrade --dedupe --force",
    "nuxiupgrade": "npx nuxi upgrade"
  },
  "devDependencies": {
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@iconify-json/lucide": "^1.2.87",
    "@nuxt/eslint": "latest",
    "@nuxt/image": "^2.0.0",
    "@nuxt/ui": "^4.4.0",
    "@nuxtjs/device": "^4.0.0",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vite-pwa/nuxt": "^1.1.0",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "^14.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-compat": "^6.1.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^10.7.0",
    "i": "^0.3.7",
    "nuxt": "^4.3.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vue": "latest",
    "vue-tsc": "^3.2.4"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ]
}
